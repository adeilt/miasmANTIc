{
  "manifest_version": 3,
  "name": "miasmANTIc",
  "version": "0.1",
  "description": "A Chrome extension that helps me efficiently browse items on Steam.",
  "author": "Wil Langford",
  "host_permissions": ["https://github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://store.steampowered.com/app/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
